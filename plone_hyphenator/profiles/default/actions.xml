<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_workflow"/>
 <action-provider name="portal_types"/>
 <action-provider name="portal_actions" />
 <object name="user" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="notifications" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Hyphenations</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:javascript:$$("html").data().plone_hyphenator.manageController.open();</property>
   <property name="icon_expr"></property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>